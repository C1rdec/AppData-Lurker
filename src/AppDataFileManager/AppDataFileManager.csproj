    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company>C1rdec</Company>
    <Authors>C1rdec</Authors>
    <PackageProjectUrl>https://github.com/C1rdec/AppDataFileManager</PackageProjectUrl>
